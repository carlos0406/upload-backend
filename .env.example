# URL de conexão para o banco de dados PostgreSQL usando PgBouncer (para pooling de conexões)
DATABASE_URL="postgresql://<usuario>:<senha>@<host>:<porta>/<banco_de_dados>?pgbouncer=true"

# URL de conexão direta para o banco de dados PostgreSQL (sem PgBouncer)
DIRECT_URL="postgresql://<usuario>:<senha>@<host>:<porta>/<banco_de_dados>"

# Chave de acesso da AWS (S3)
ACCESS_KEY_ID="<your_aws_access_key_id>"

# Chave secreta de acesso da AWS (S3)
SECRET_ACCESS_KEY="<your_aws_secret_access_key>"

# URL para download dos arquivos no bucket S3
BUCKET_DOWNLOAD_URL="<your_bucket_download_url>"

# Endpoint do bucket S3 (pode ser fornecido pelo serviço ou customizado)
BUCKET_ENDPOINT="<your_bucket_endpoint_url>"

# Segredo para assinar e validar cookies (usado para sessões seguras)
COOKIE_SECRET="<your_cookie_secret>"

# Segredo JWT para autenticação segura
JWT_SECRET="<your_jwt_secret>"

# Chave privada para autenticação (pode ser usada para assinar tokens ou comunicação segura)
PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
<your_private_key>
-----END PRIVATE KEY-----"

# Chave pública para autenticação (usada para validar tokens ou comunicação segura)
PUBLIC_KEY="-----BEGIN PUBLIC KEY-----
<your_public_key>
-----END PUBLIC KEY-----"